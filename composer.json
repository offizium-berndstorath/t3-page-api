{
  "name": "offizium/t3pageapi",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "description": "Create Pages using the Api",
  "require": {
    "typo3/cms-core": "^12.4",
    "nng/nnrestapi": "^2.0",
    "nng/nnhelpers": "^2.0",
    "typo3/cms-extbase": "^12.4",
    "ext-json": "*"
  },
    "autoload": {
    "psr-4": {
      "Offizium\\T3pageapi\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Offizium\\T3pageapi\\Tests\\": "Tests"
    }
  },
  "extra": {
    "typo3/cms": {
       "extension-key": "t3pageapi"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
