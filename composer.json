{
  "name": "offizium/t3-page-api",
  "version": "1.0.0",
  "type": "typo3-cms-extension",
  "description": "Create Pages using the Api",
  "require": {
    "php": ">=8.0",
    "typo3/cms-core": "^12.4.0",
    "typo3/cms-fluid-styled-content": "^12.4.0",
    "sourcebroker/t3api": "^3.0"
  },
  "license": "MIT",
  "keywords": [
    "typo3",
    "site package",
    "api"
  ],
  "autoload": {
    "psr-4": {
      "offizium\\T3pageapi\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "t3_page_api"
    },
    "branch-alias": {
      "dev-master": "12.0.x-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/cms-composer-installers": true,
      "typo3/class-alias-loader": true
    }
  }
}
